                <script>
                  $(document).ready(function () {

                    $(".allergencheck").change(function () {
                      if (this.checked) {
                        $(this).attr("aria-checked", "true")
                      } else {
                        $(this).attr("aria-checked", "false")
                      }
                    });

                    $("#filter2").change(function () {
                      if (this.checked) {
                        $(this).attr("aria-checked", "true")
                        $("#filter1").attr("aria-checked", "false")
                      }
                    });


                    $("#filter1").change(function () {
                      if (this.checked) {
                        $(this).attr("aria-checked", "true")
                        $("#filter2").attr("aria-checked", "false")
                      }
                    });


                  });
                </script>






