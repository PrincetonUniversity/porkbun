<!-- partials are elements common to all pages -->
<% include ../partials/header %>
   <div class="container">
      <h1>PorkBun</h1>
      <% if (netid) { %>
         
         <form action="/addprefs" method="post">
            <label for="dish">Add a dish to your preferences:</label>
            <input type="text" placeholder="Pork Buns" name="dish" id="dish">
            <input type="submit">
         </form>
   
      <% } else { %>
            <a href="/auth/login">Login</a>
      <% } %>
   </div> 
<% include ../partials/footer %>